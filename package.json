{
  "name": "react-consent-root",
  "private": true,
  "version": "0.0.0",
  "packageManager": "pnpm@7.9.3",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "build": "turbo run build",
    "clean": "turbo run clean && rm -rf node_modules/ && yarn",
    "dev": "turbo run dev --no-cache --parallel --continue",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\""
  },
  "devDependencies": {
    "eslint": "8.23.1",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "pretty-quick": "3.1.3",
    "shell-quote": "1.7.3",
    "turbo": "1.5.4"
  },
  "dependencies": {
    "@changesets/changelog-github": "^0.4.7",
    "@changesets/cli": "^2.25.2"
  }
}
